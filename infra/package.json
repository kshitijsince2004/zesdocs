{
  "name": "@zesdocs/infra",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:start": "bash scripts/start.sh",
    "dev:stop": "bash scripts/stop.sh",
    "backup": "bash scripts/backup.sh",
    "status": "docker-compose -f docker-compose.dev.yml ps",
    "es:index": "node scripts/create-es-index.js"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0"
  }
}
